     <checklist>
       <title>Engine Startup</title>
       <item>
	 <name>APU</name>
	 <value>check power established</value>
	 <condition>
	   <and>
	     <greater-than-equals>
	       <property>/engines/engine[2]/rpm</property>
	       <value>75</value>
	     </greater-than-equals>
	     <greater-than-equals>
	       <property>/systems/electrical/outputs/FCS</property>
	       <value>105</value>
	     </greater-than-equals>
	   </and>
	 </condition>
       </item>
       <item>
	<name>Engine Start Switch</name>
	<value>set ON, wait for CYCLE</value>
	<condition>
	  <and>
	    <greater-than-equals>
	      <property>/engines/engine[0]/n1</property>
	      <value>34</value>
	    </greater-than-equals>
	    <greater-than-equals>
	      <property>/engines/engine[1]/n1</property>
	      <value>34</value>
	    </greater-than-equals>
	  </and>
	</condition>
	<marker>
	  <x-m>3.8333</x-m>
	  <y-m>0.4533</y-m>
	  <z-m>0.6425</z-m>
	  <scale>1.8833</scale>
	</marker>
	<binding>
	  <command>nasal</command>
	  <script>avionics.controls.engineStart(1);</script>
	</binding>
      </item>
 
